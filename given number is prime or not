#include<stdio.h>
void main()
{
    int i,n,Count;
    printf("Enter n value:");
    scanf("%d",&n);
    Count=0;
    for(i=1;i<=n;++i)
    {
        if(n%i==0)
        {
            Count++;
        }}
        if(Count==2)
        {
            printf("prime");
        }
        else
        {
            printf("Not prime");
        }
        }
        
